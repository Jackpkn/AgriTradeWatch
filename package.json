{
  "name": "agritradewatch",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "adb uninstall com.devsingal.Agritradewatch && npx expo run:android",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "prebuild": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "build": "cd android && gradlew assembleDebug && cd ../",
    "clear": "cd android && gradlew clean && cd ../"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/cli": "^0.24.20",
    "@expo/vector-icons": "^14.1.0",
    "@gorhom/bottom-sheet": "^5.2.3",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/cli-platform-android": "^16.0.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-community/slider": "4.5.6",
    "@react-native-picker/picker": "2.11.1",
    "@react-native/gradle-plugin": "^0.77.0",
    "@sentry/react-native": "~6.14.0",
    "expo": "53.0.22",
    "expo-build-properties": "~0.14.8",
    "expo-camera": "~16.1.11",
    "expo-constants": "~17.1.7",
    "expo-font": "~13.3.2",
    "expo-image-manipulator": "~13.1.7",
    "expo-image-picker": "~16.1.4",
    "expo-linear-gradient": "~14.1.5",
    "expo-linking": "~7.1.7",
    "expo-location": "~18.1.6",
    "expo-media-library": "~17.1.7",
    "expo-router": "~5.1.5",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.11",
    "expo-web-browser": "~14.2.0",
    "firebase": "^11.0.2",
    "leaflet": "^1.9.4",
    "metro": "^0.82.5",
    "metro-config": "^0.82.5",
    "metro-resolver": "^0.82.5",
    "react": "19.0.0",
    "react-dom": "^19.0.0",
    "react-native": "^0.79.5",
    "react-native-async-storage": "^0.0.1",
    "react-native-dotenv": "^3.4.11",
    "react-native-geolocation-service": "^5.3.1",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-gifted-charts": "^1.4.55",
    "react-native-leaflet-view": "^1.1.2",
    "react-native-paper": "^5.12.5",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-vector-icons": "^10.2.0",
    "react-native-web": "^0.20.0",
    "react-native-webview": "13.13.5",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.25.8",
    "@react-native-community/cli": "latest",
    "@types/jest": "^29.5.13",
    "@types/react": "~19.0.10",
    "@types/react-test-renderer": "^18.3.0",
    "jest": "^29.7.0",
    "jest-expo": "~53.0.10",
    "tailwindcss": "^3.4.13",
    "typescript": "~5.8.3"
  },
  "private": true,
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "react-native-geolocation-service",
          "@react-native-community/cli-platform-android",
          "@react-native/gradle-plugin",
          "firebase",
          "react-native-async-storage",
          "react-native-vector-icons",
          "uuid"
        ],
        "listUnknownPackages": false
      }
    }
  }
}
