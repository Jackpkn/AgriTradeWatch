{
  "name": "agritradewatch",
  "main": "index.js",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "reinstall": "adb uninstall com.devsingal.Agritradewatch && npx expo run:android",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "prebuild": "npx expo prebuild --platform android",
    "build:dev": "npx expo run:android",
    "build:preview": "eas build --platform android --profile preview",
    "build:production": "eas build --platform android --profile production",
    "clean": "cd android && ./gradlew clean && cd ../",
    "eas:build": "eas build --platform android --profile preview"
  },
  "jest": {
    "preset": "jest-expo"
  },
