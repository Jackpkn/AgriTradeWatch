{
  "name": "agritradewatch",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "adb uninstall com.devsingal.Agritradewatch && npx expo run:android",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint",
