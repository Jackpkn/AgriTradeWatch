{
  "name": "agritradewatch",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "reinstall": "adb uninstall com.devsingal.Agritradewatch && npx expo run:android",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "prebuild": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "build": "cd android && gradlew assembleDebug && cd ../",
    "clear": "cd android && gradlew clean && cd ../"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/cli": "^0.24.20",
    "@expo/metro-runtime": "~6.1.2",
    "@expo/vector-icons": "^15.0.2",
    "@gorhom/bottom-sheet": "^4.6.4",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/cli-platform-android": "^16.0.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-community/slider": "5.0.1",
    "@react-native-picker/picker": "2.11.1",
    "@react-native/gradle-plugin": "^0.77.0",
    "@sentry/react-native": "~6.20.0",
    "expo": "^54.0.8",
    "expo-build-properties": "~1.0.8",
    "expo-camera": "~17.0.8",
    "expo-constants": "~18.0.9",
    "expo-font": "~14.0.8",
    "expo-image-manipulator": "~14.0.7",
    "expo-image-picker": "~17.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.8",
    "expo-location": "~19.0.7",
    "expo-media-library": "~18.2.0",
    "expo-router": "~6.0.6",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-system-ui": "~6.0.7",
    "expo-web-browser": "~15.0.7",
    "firebase": "^12.2.1",
    "metro": "^0.83.1",
    "metro-config": "^0.82.5",
    "metro-resolver": "^0.82.5",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-geolocation-service": "^5.3.1",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-gifted-charts": "^1.4.55",
    "react-native-paper": "^5.12.5",
    "react-native-reanimated": "~4.1.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-vector-icons": "^10.2.0",
    "react-native-web": "^0.21.0",
    "react-native-webview": "13.15.0",
    "react-native-worklets": "^0.5.0",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.25.8",
    "@react-native-community/cli": "latest",
    "@types/jest": "^29.5.13",
    "@types/react": "~19.1.10",
    "@types/react-test-renderer": "^18.3.0",
    "jest": "^29.7.0",
    "jest-expo": "~54.0.11",
    "tailwindcss": "^3.4.13",
    "typescript": "~5.9.2"
  },
  "private": true,
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "react-native-geolocation-service",
          "@react-native-community/cli-platform-android",
          "@react-native/gradle-plugin",
          "react-native-async-storage",
          "react-native-vector-icons",
          "uuid"
        ],
        "listUnknownPackages": false
      }
    }
  }
}
